{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "destination": "index.js"
        }
    ]
    
  "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }]

}
